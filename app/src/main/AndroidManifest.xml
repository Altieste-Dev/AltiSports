<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.altisportss">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="br.com.altisportss.view.ProdutoActivity" android:label="@string/gerenciar_produtos">
        </activity>
        <activity
            android:name="br.com.altisportss.view.CarrinhoItemActivity"
            android:label="@string/adicionando_produto" />
        <activity
            android:name="br.com.altisportss.view.MainActivity"
            android:label="@string/meu_carrinho">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.JogadoresActivity"></activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
</manifest>